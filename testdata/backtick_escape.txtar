Backtick escape producing literal string.

-- values.yaml --
name: test
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .Values.name }}
data:
  key: {{`hello`}}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  key: hello
-- output.cue --
#values: {
	name!: bool | number | string | null
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: name: #values.name
data: key: "hello"
