Cross-document range block: {{range}} wraps --- separator.
No round-trip: CUE wraps documents in separate fields.

-- _helpers.tpl --
-- values.yaml --
items:
  - name: a
  - name: b
-- input.yaml --
{{- range .Values.items }}
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .name }}
data:
  key: one
---
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .name }}
data:
  key: two
{{- end }}
-- output.cue --
#values: {
	items!: _
	...
}

document_0: {
	for _, _range0 in #values.items {
		apiVersion: "v1"
		kind:       "ConfigMap"
		metadata: name: _range0.name
		data: key: "one"
	}
}
document_1: {
	for _, _range0 in #values.items {
		apiVersion: "v1"
		kind:       "ConfigMap"
		metadata: name: _range0.name
		data: key: "two"
	}
}
