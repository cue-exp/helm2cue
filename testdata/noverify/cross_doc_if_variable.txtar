Cross-document if block with variable defined before split.
No round-trip: CUE wraps documents in separate fields.

-- _helpers.tpl --
-- values.yaml --
enabled: true
name: test
-- input.yaml --
{{- $myName := .Values.name }}
{{- if .Values.enabled }}
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ $myName }}-one
data:
  key: one
---
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ $myName }}-two
data:
  key: two
{{- end }}
-- output.cue --
import "struct"

#values: {
	name!:    bool | number | string | null
	enabled?: bool | number | string | null
	...
}

document_0: {
	if (_nonzero & {#arg: #values.enabled, _}) {
		apiVersion: "v1"
		kind:       "ConfigMap"
		metadata: name: "\(#values.name)-one"
		data: key: "one"
	}
}
document_1: {
	if (_nonzero & {#arg: #values.enabled, _}) {
		apiVersion: "v1"
		kind:       "ConfigMap"
		metadata: name: "\(#values.name)-two"
		data: key: "two"
	}
}
_nonzero: {
	#arg?: _
	[if #arg != _|_ {
		[
			if (#arg & int) != _|_ {#arg != 0},
			if (#arg & string) != _|_ {#arg != ""},
			if (#arg & float) != _|_ {#arg != 0.0},
			if (#arg & bool) != _|_ {#arg},
			if (#arg & [...]) != _|_ {len(#arg) > 0},
			if (#arg & {...}) != _|_ {(#arg & struct.MaxFields(0)) == _|_},
			false,
		][0]
	}, false][0]
}
