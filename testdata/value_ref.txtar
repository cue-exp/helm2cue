Value references: {{ .Values.x }} should become #values.x CUE references.

-- values.yaml --
name: my-app
replicas: 3
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .Values.name }}
data:
  replicas: {{ .Values.replicas }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: my-app
data:
  replicas: 3
-- output.cue --
#values: {
	name!:     bool | number | string | null
	replicas!: bool | number | string | null
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: {
	name: #values.name
}
data: {
	replicas: #values.replicas
}
