String trim function.

-- values.yaml --
name: "  my-app  "
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  trimmed: {{ .Values.name | trim }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  trimmed: my-app
-- output.cue --
import "strings"

#values: {
	name!: bool | number | string | null
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: name: "test"
data: trimmed: strings.TrimSpace(#values.name)
