default with include call as default value argument.

-- _helpers.tpl --
{{- define "myapp.namespace" -}}
default-ns
{{- end -}}

-- values.yaml --
{}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  namespace: default-ns
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  namespace: {{ default (include "myapp.namespace" .) .Values.namespace }}
-- output.cue --
#values: {
	namespace?: bool | number | string | null
	...
}
_myapp_namespace: "default-ns"

output: [
	{
		apiVersion: "v1"
		kind:       "ConfigMap"
		metadata: namespace: *#values.namespace | _myapp_namespace
	},
]
