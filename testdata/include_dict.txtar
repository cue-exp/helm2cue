Include with dict context: field references from dict values are tracked.

-- input.yaml --
apiVersion: v1
kind: Pod
metadata:
  name: test
spec:
  image: {{ include "common.images.image" (dict "imageRoot" .Values.image "global" .Values.global) }}
-- output.cue --
#values: {
	image!:  bool | number | string | null
	global!: bool | number | string | null
	...
}
_common_images_image: _

apiVersion: "v1"
kind:       "Pod"
metadata: name: "test"
spec: image: _common_images_image
