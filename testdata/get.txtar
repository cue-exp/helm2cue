get function: access a map value by key.

-- values.yaml --
labels:
  app: myapp
  version: v1
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  app: {{ get .Values.labels "app" }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  app: myapp
-- output.cue --
#values: {
	labels?: _
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: {
	name: "test"
}
data: {
	app: #values.labels.app
}
