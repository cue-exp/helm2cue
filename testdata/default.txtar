Pipeline with default value.

-- values.yaml --
name: my-app
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .Values.name | default "fallback" }}
data:
  env: {{ .Values.env | default "production" }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: my-app
data:
  env: production
-- output.cue --
#values: {
	name: *"fallback" | bool | number | string | null
	env:  *"production" | bool | number | string | null
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: name: #values.name
data: env: #values.env
