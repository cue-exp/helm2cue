Pipeline with default value.

-- values.yaml --
name: my-app
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .Values.name | default "fallback" }}
data:
  env: {{ .Values.env | default "production" }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: my-app
data:
  env: production
-- output.cue --
#values: {
	name?: bool | number | string | null
	env?:  bool | number | string | null
	...
}

output: [
	{
		apiVersion: "v1"
		kind:       "ConfigMap"
		metadata: name: *#values.name | "fallback"
		data: env: *#values.env | "production"
	},
]
