Range over a list, producing list items.

-- values.yaml --
items:
  - one
  - two
  - three
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  items:
  {{- range .Values.items }}
    - {{ . }}
  {{- end }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  items:
    - one
    - two
    - three
-- output.cue --
#values: {
	items?: _
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: {
	name: "test"
}
data: {
	items: [
		for _, _range0 in #values.items {
			_range0
		},
	]
}
