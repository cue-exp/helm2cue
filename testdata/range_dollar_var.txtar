Range over $.Values (dollar prefix).

-- values.yaml --
items:
  - one
  - two
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  items:
  {{- range $.Values.items }}
    - {{ . }}
  {{- end }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  items:
    - one
    - two
-- output.cue --
#values: {
	items!: _
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: name: "test"
data: items: [
	for _, _range0 in #values.items {
		_range0
	},
]
